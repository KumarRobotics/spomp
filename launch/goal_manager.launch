<?xml version="1.0" encoding="UTF-8"?>
<launch>
  <arg name="find_obj" default="False"/>
  <arg name="robot_list" default="europa,callisto,io"/>

  <node pkg="spomp" type="goal_manager.py" name="goal_manager" output="screen">
    <!-- Remappings -->
    <remap from="~add_goal_point" to="/clicked_point"/>
    <remap from="~pose" to="/os_node/llol_odom/pose"/>
    <remap from="~target_goals" to="/groundstation/target_goals"/>

    <!-- Params -->
    <param name="~robot_list" value="$(arg robot_list)"/>
    <param name="~this_robot" value="$(env ROBOT_NAME)"/>
  </node>

  <node pkg="spomp" type="find_object_goal_gen.py" name="find_object_goal_gen" output="screen" if="$(arg find_obj)"/>
</launch>
